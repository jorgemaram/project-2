        <div class="col-md-6">
            <div class="app-search">
                <div class="input-container">
                <input type="text" placeholder="Busca a un usuario" v-model="authorNameSearchString"/>
                </div>
            </div>
            <transition-group tag="ul" name="list-animation">
                <li class="photo photo-animated" v-for="photo in filteredPhotoFeed" v-bind:key="user.id">
